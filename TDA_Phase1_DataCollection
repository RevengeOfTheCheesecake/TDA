TC', 'AMD', 'KO', 'WMT'\n","]\n","\n","print(f\"\\nüìä Universe: {len(universe)} stocks\")\n","print(f\"Tickers: {', '.join(universe)}\\n\")\n","\n","# Download 5 years of data\n","start_date = '2019-01-01'\n","end_date = '2024-12-10'\n","\n","print(f\"üìÖ Downloading data from {start_date} to {end_date}...\")\n","print(\"Using alternative method...\\n\")\n","\n","# METHOD 2: Use Ticker objects directly (more reliable)\n","prices_dict = {}\n","failed_tickers = []\n","\n","for ticker in universe:\n","    try:\n","        print(f\"Downloading {ticker}...\", end=\" \")\n","        stock = yf.Ticker(ticker)\n","        hist = stock.history(start=start_date, end=end_date)\n","\n","        if not hist.empty and 'Close' in hist.columns:\n","            prices_dict[ticker] = hist['Close']\n","            print(\"‚úÖ\")\n","        else:\n","            print(\"‚ùå No data\")\n","            failed_tickers.append(ticker)\n","    except Exception as e:\n","        print(f\"‚ùå Error: {str(e)[:50]}\")\n","        failed_tickers.append(ticker)\n","\n","# Combine into DataFrame\n","prices = pd.DataFrame(prices_dict)\n","\n","# Fill any gaps (forward fill then backward fill)\n","prices = prices.fillna(method='ffill').fillna(method='bfill')\n","\n","# Calculate daily returns\n","returns = prices.pct_change().dropna()\n","\n","print(\"\\n‚úÖ Download complete!\\n\")\n","print(\"=\" * 70)\n","print(\"DATA SUMMARY\")\n","print(\"=\" * 70)\n","print(f\"Date range: {prices.index[0].date()} to {prices.index[-1].date()}\")\n","print(f\"Trading days: {len(prices)}\")\n","print(f\"Stocks successfully downloaded: {len(prices.columns)}\")\n","if failed_tickers:\n","    print(f\"‚ö†Ô∏è  Failed tickers: {', '.join(failed_tickers)}\")\n","print(f\"Missing data points after filling: {prices.isna().sum().sum()}\")\n","\n","print(\"\\nüìà Sample prices (last 5 days):\")\n","print(prices.tail())\n","\n","print(\"\\nüìä Sample returns (last 5 days):\")\n","print(returns.tail().round(4))\n","\n","print(\"\\nüìä Basic statistics:\")\n","print(f\"Average daily return: {returns.mean().mean():.4%}\")\n","print(f\"Average daily volatility: {returns.std().mean():.4%}\")\n","\n","# Save to CSV\n","prices.to_csv('stock_prices.csv')\n","returns.to_csv('stock_returns.csv')\n","\n","print(\"\\nüíæ Data saved to:\")\n","print(\"  - stock_prices.csv\")\n","print(\"  - stock_returns.csv\")\n","\n","# Quick correlation check\n","corr_matrix = returns.corr()\n","print(f\"\\nüîó Average correlation: {corr_matrix.values[np.triu_indices_from(corr_matrix.values, k=1)].mean():.3f}\")\n","\n","print(\"\\n‚úÖ Phase 1 Complete! Ready for Phase 2: Graph Construction\")\n","print(\"=\" * 70)"]}]}